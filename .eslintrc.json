{
  "extends": [ "canonical/auto", "next/core-web-vitals" ],
  "rules": {
    "canonical/filename-match-exported": [ 2, { "transforms": [ "pascal", "snake" ] } ],
    "import/no-unassigned-import": [ 2, { "allow": [ "*/styles/*.css" ] } ],
    "react/forbid-component-props": [ 2, { "forbid": [ {
            "propName": "className",
            "allowedFor": [ "Link", "Image" ],
            "message": "Prop className is forbidden on components except <Link> and <Image>"
          } ] } ]
  }
}
